  "scripts": {
    "postinstall": "node patch-webpack.js",
    "ng": "ng",
    "start": "ng serve admin-dashboard",
    "build": "npm run build:dashboard",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start:dashboard": "ng serve admin-dashboard --configuration=development",
    "start:dashboard:production": "ng serve admin-dashboard --configuration=production",
    "start:app": "ng serve --port 4200",
    "start:dashboard-mock": "cd projects/admin-dashboard-backend-mock && node index.js",
    "build:dashboard:production": "ng build admin-dashboard --prod --aot",
    "build:dashboard:staging": "ng build admin-dashboard -c staging",
    "build:dashboard:develop": "ng build admin-dashboard --dev --source-map -ec true",
    "build:app": "ng build --prod --aot",
    "build:style": "ng build style-lib",
    "liteserver:dashboard": "lite-server --baseDir=\"dist/admin-dashboard\"",
    "liteserver:app": "lite-server --baseDir=\"dist/dashboard\"",
    "deploy:prepare:sta": "firebase target:apply hosting sta ${FIREBASE_TARGET_STA}",
    "deploy:prepare:prd": "firebase target:apply hosting prd ${FIREBASE_TARGET_PRD}",
    "deploy:ci:sta": "firebase deploy --only hosting:sta --token $FIREBASE_TOKEN_STA",
    "deploy:ci:prd": "firebase deploy --only hosting:prd --token $FIREBASE_TOKEN_PRD"
  },
  "private": true,
  "browser": {
    "path": false,
    "fs": false
  },
  "dependencies": {
    "@agm/core": "^1.1.0",
    "@angular-devkit/build-angular": "~0.901.7",
